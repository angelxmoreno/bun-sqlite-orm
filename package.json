{
  "name": "type-bun-orm",
  "version": "1.0.0",
  "description": "A lightweight TypeScript ORM for Bun runtime",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun",
    "dev": "bun run src/index.ts",
    "test": "bun test",
    "typecheck": "bun --bun tsc --noEmit"
  },
  "keywords": ["orm", "typescript", "bun", "database"],
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}